{
  "name": "react-cognito-material",
  "version": "0.2.8",
  "description": "Ties together ReactJS with AWS Cognito identity, using Material UI for styling",
  "license": "MIT",
  "repository": "https://github.com/klumhru/react-cognito-material",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^15.6.1"
  },
  "dependencies": {
    "amazon-cognito-auth-js": "^1.0.0",
    "amazon-cognito-identity-js": "^1.25.0",
    "aws-sdk": "^2.150.0",
    "material-ui": "^1.0.0-beta.21",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^2.6.1",
    "xregexp": "^3.2.0"
  },
  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "node scripts/test.js --coverage --env=jsdom --watch",
    "start": "webpack --watch",
    "build": "webpack"
  },
  "author": {
    "name": "HÃ¶gni Gylfason",
    "email": "klumhru@gmail.com",
    "website": "https://klumhru.com"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": [
          "scripts/**/*.js",
          "config/**/*.js"
        ],
        "excludedFiles": "*.js"
      }
    ],
    "extends": "airbnb",
    "env": {
      "jest": true
    },
    "globals": {
      "document": true,
      "window": true
    },
    "rules": {
      "object-curly-newline": [
        0
      ],
      "no-plusplus": 0,
      "no-console": 0,
      "no-confusing-arrow": [
        0
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "react/forbid-prop-types": [
        0
      ],
      "import/extensions": [
        2,
        {
          "js": "never",
          "jsx": "never"
        }
      ],
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "semi": [
        2,
        "never"
      ],
      "arrow-parens": [
        2,
        "always"
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.7",
    "css.escape": "^1.5.1",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "jss": "^8.1.0",
    "postcss-loader": "^2.0.8",
    "react-test-renderer": "^16.1.1"
  }
}
