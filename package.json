{
  "name": "react-cognito-material",
  "version": "0.0.1",
  "description": "Ties together ReactJS with AWS Cognito identity, using Material UI for styling",
  "license": "MIT",
  "repository": "https://github.com/klumhru/react-cognito-material",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^15.6.1"
  },
  "dependencies": {
    "amazon-cognito-auth-js": "^1.0.0",
    "amazon-cognito-identity-js": "^1.21.0",
    "aws-sdk": "^2.133.0",
    "babel-jest": "20.0.3",
    "jest": "20.0.4",
    "jss": "^8.1.0",
    "material-ui": "^1.0.0-beta.17",
    "react": "^15.6.1",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^2.6.1",
    "xregexp": "^3.2.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --watch",
    "build": "webpack"
  },
  "author": {
    "name": "HÃ¶gni Gylfason",
    "email": "klumhru@gmail.com",
    "website": "https://klumhru.com"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "jest": true
    },
    "globals": {
      "document": true,
      "window": true
    },
    "rules": {
      "no-console": 0,
      "no-confusing-arrow": [
        0
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "react/forbid-prop-types": [
        0
      ],
      "import/extensions": [
        2,
        {
          "js": "never",
          "jsx": "never"
        }
      ],
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "semi": [
        2,
        "never"
      ],
      "arrow-parens": [
        2,
        "always"
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react-app": "^3.0.2"
  }
}
